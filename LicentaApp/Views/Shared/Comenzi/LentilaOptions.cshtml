@using LicentaApp.Domain
@model LicentaApp.ViewModels.ComandaViewModel


@if (ViewData.ContainsKey(AppConstants.LentilaOptions))
{
    <div class="row">
        <div class="col m12 input-field">
            @Html.DropDownListFor(model => model.Lentila.CodProdus, (SelectList)ViewData[AppConstants.LentilaOptions], "- Selecteaza -", new { @id = "lentila-options" })
            @Html.LabelFor(model => model.Lentila.CodProdus, "Denumire Lentila")
            @Html.ValidationMessageFor(model => model.Lentila.CodProdus, "", new { @class = "text-danger" })
        </div>
    </div>
}
