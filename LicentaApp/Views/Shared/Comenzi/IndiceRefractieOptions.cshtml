@using LicentaApp.Domain
@model LicentaApp.ViewModels.ComandaViewModel

@if (ViewData.ContainsKey(AppConstants.IndiceReractieOptions))
{
    <div class="row">
        <div class="col m12 input-field">
            @Html.DropDownListFor(model => model.Lentila.IndiceRefractie, (SelectList)ViewData[AppConstants.IndiceReractieOptions], "- Selecteaza -", new { @id = "indice-refractie-options" })
            @Html.LabelFor(model => model.Lentila.IndiceRefractie)
            @Html.ValidationMessageFor(model => model.Lentila.IndiceRefractie, "", new { @class = "text-danger" })
        </div>
    </div>
}
