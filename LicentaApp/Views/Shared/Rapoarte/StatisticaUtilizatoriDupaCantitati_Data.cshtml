@model System.Data.Entity.Core.Objects.ObjectResult<GetStatisticaUtilizatoriDupaCantitati_Result>

<table class="highlight">
    <thead>
    <tr>
        <th>Nume</th>
        <th>Cantitate totala</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in this.Model)
    {
        <tr>
            <td>@item.Nume @item.Prenume</td>
            <td>@item.CantitateTotala</td>
        </tr>
    }
    </tbody>
</table>