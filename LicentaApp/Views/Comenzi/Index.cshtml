@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Data)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Discount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Clienti.Nume)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Utilizatori.Username)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ViziteMedicale.IdComandaVizitaMedicala)
